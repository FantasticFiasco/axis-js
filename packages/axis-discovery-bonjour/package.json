{
  "name": "axis-discovery-bonjour",
  "version": "2.0.0",
  "description": "A Node.js Bonjour client library written in TypeScript capable of searching for Axis Communication cameras.",
  "keywords": [
    "axis",
    "communications",
    "camera",
    "device",
    "discovery",
    "bonjour",
    "zero-configuration"
  ],
  "homepage": "https://github.com/FantasticFiasco/axis-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/FantasticFiasco/axis-js.git"
  },
  "bugs": {
    "url": "https://github.com/FantasticFiasco/axis-js/issues"
  },
  "author": "Mattias Kindborg <mattias.kindborg@gmail.com> (https://twitter.com/FantasticFiasco)",
  "license": "Apache-2.0",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "src/**/*.js",
    "src/**/*.d.ts",
    "vendor/**/*.js",
    "vendor/**/*.d.ts"
  ],
  "scripts": {
    "clean": "rimraf \"./+(src|test)/**/*.+(d.ts|d.ts.map|js|js.map)\" ./coverage",
    "build": "yarn clean && tsc",
    "start": "yarn build && node ./sample/index.js",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "coverage": "jest --coverage && coveralls < coverage/lcov.info",
    "prepack": "yarn build",
    "preversion": "git add ./CHANGELOG.md",
    "postversion": "git push --follow-tags",
    "merge-dependencies": "node ./build/merge-dependencies.js"
  },
  "dependencies": {
    "@fantasticfiasco/expect": "1.1.0",
    "array-flatten": "^2.1.0",
    "debug": "4.3.2",
    "deep-equal": "^1.0.1",
    "dns-equal": "^1.0.0",
    "dns-packet": "^1.0.1",
    "dns-txt": "^2.0.2",
    "multicast-dns-service-types": "^1.1.0",
    "thunky": "^0.1.0"
  },
  "devDependencies": {
    "@types/debug": "4.1.7",
    "@types/jest": "27.0.2",
    "@types/node": "14.17.27",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "coveralls": "3.1.1",
    "eslint": "7.32.0",
    "jest": "27.3.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.7",
    "typescript": "4.4.4"
  }
}
